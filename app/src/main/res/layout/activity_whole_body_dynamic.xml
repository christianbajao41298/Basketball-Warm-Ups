<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Stretching">

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="44dp"
        android:text="@string/whole_body_dynamic"
        android:textColor="#020202"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="525dp"
        android:layout_height="250dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19"
        app:srcCompat="@drawable/wholebodypreview"
        android:contentDescription="@string/imagePreview" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="33dp"
        android:text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Maecenas et consectetur ante. Vivamus vulputate nibh eros, vel tempor magna posuere sed. Sed fermentum tortor tellus, tempor molestie lorem fringilla non."
        android:textAlignment="center"
        android:textColor="#050505"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView19" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="393dp"
        android:layout_height="466dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:fillViewport="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageButton
                android:id="@+id/armStretchLeftButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/armstretchleftbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/armStretchRightButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/armStretchLeftButtonIcon"
                app:srcCompat="@drawable/armstretchrghtbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/kneeHugsLeftButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/armCirclesButtonIcon"
                app:srcCompat="@drawable/kneehugsleftbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/kneeHugsRightButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/kneeHugsLeftButtonIcon"
                app:srcCompat="@drawable/kneehugsrightbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/armCirclesButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/armStretchRightButtonIcon"
                app:srcCompat="@drawable/armcirclesbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/jumpingJacksButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/kneeHugsRightButtonIcon"
                app:srcCompat="@drawable/jumpingjacksbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/kneeStretchLeftButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/jumpingJacksButtonIcon"
                app:srcCompat="@drawable/kneestretchleftbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/kneeStretchRightButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/kneeStretchLeftButtonIcon"
                app:srcCompat="@drawable/kneestretchrightbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/buttKicksButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/kneeStretchRightButtonIcon"
                app:srcCompat="@drawable/buttkicksbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/walkingHighKneesButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/buttKicksButtonIcon"
                app:srcCompat="@drawable/walkinghighkneesbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/sideSlideButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/walkingHighKneesButtonIcon"
                app:srcCompat="@drawable/sideslidebuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/lungesButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/sideSlideButtonIcon"
                app:srcCompat="@drawable/lungesbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/frankMarchButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lungesButtonIcon"
                app:srcCompat="@drawable/frankmarchbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/squatButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/frankMarchButtonIcon"
                app:srcCompat="@drawable/squatbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/inchWormButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/squatButtonIcon"
                app:srcCompat="@drawable/inchwormbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/walkingLungeButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inchWormButtonIcon"
                app:srcCompat="@drawable/walkinglungebuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/gluteWalkButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/walkingLungeButtonIcon"
                app:srcCompat="@drawable/glutewalkbuttonicon"
                android:contentDescription="@string/imageButton" />

            <ImageButton
                android:id="@+id/pushUpButtonIcon"
                android:layout_width="350dp"
                android:layout_height="90dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="150dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/gluteWalkButtonIcon"
                app:srcCompat="@drawable/pushupbuttonicon"
                android:contentDescription="@string/imageButton" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>